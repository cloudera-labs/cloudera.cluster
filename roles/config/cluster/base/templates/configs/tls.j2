    dfs_data_transfer_protection: privacy
    hadoop_group_mapping_ldap_keystore: {{ tls_keystore_path_generic }}
    hadoop_group_mapping_ldap_keystore_passwd: {{ tls_keystore_password }}
    hadoop_group_mapping_ldap_use_ssl: True
    hdfs_hadoop_ssl_enabled: True
    ssl_client_truststore_location: {{ tls_truststore_path }}
    ssl_client_truststore_password: {{ tls_truststore_password }}
    ssl_server_keystore_keypassword: {{ tls_keystore_password }}
