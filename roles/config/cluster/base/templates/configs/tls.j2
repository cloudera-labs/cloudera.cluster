---
FLUME:
  SERVICEWIDE:
    flume_truststore_file: {{ tls_truststore_path }}
    flume_truststore_password: {{ tls_truststore_password }}
HBASE:
  SERVICEWIDE:
    hbase_hadoop_ssl_enabled: True
    ssl_server_keystore_keypassword: {{ tls_keystore_password }}
    ssl_server_keystore_location: {{ tls_keystore_path_generic }}
    ssl_server_keystore_password: {{ tls_keystore_password }}
  HBASERESTSERVER:
    hbase_restserver_keystore_file: {{ tls_keystore_path_generic }}
    hbase_restserver_keystore_keypassword: {{ tls_keystore_password }}
    hbase_restserver_keystore_password: {{ tls_keystore_password }}
