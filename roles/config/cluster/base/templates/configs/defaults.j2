---
HDFS:
  DATANODE:
    dfs_data_dir_list: /dfs/dn
  JOURNALNODE:
    dfs_journalnode_edits_dir: /dfs/jn
  NAMENODE:
    dfs_name_dir_list: /dfs/nn
  SECONDARYNAMENODE:
    fs_checkpoint_dir_list: /dfs/snn
KAFKA:
  KAFKA_BROKER:
    log.dirs: /var/local/kafka/data
KUDU:
  KUDU_MASTER:
    fs_data_dirs: /kudu/master/data
    fs_wal_dir: /kudu/master/wal
  KUDU_TSERVER:
    fs_data_dirs: /kudu/tablet/data
    fs_wal_dir: /kudu/tablet/wal
OZONE:
  OZONE_DATANODE:
    ozone.scm.datanode.id.dir: /var/lib/hadoop-ozone/datanode
    dfs.container.ratis.datanode.storage.dir: /var/lib/hadoop-ozone/datanode/ratis/data
    hdds.datanode.dir: /var/lib/hadoop-ozone/datanode/data
    ozone.metadata.dirs: /var/lib/hadoop-ozone/datanode/ozone-metadata
  OZONE_MANAGER:
    ozone.om.db.dirs: /var/lib/hadoop-ozone/datanode/ozone-metadata
    ozone.metadata.dirs: /var/lib/hadoop-ozone/om/ozone-metadata
    ozone.om.ratis.storage.dir: /var/lib/hadoop-ozone/om/ratis
  OZONE_PROMETHEUS:
    ozone.prometheus.db.dir: /var/lib/hadoop-ozone/prometheus/data
  OZONE_RECON:
    ozone.recon.db.dir: /var/lib/hadoop-ozone/recon/data
    ozone.recon.scm.db.dirs: /var/lib/hadoop-ozone/recon/scm/data
    ozone.recon.om.db.dir: /var/lib/hadoop-ozone/recon/om/data
    ozone.metadata.dirs: /var/lib/hadoop-ozone/recon/ozone-metadata
STORAGE_CONTAINER_MANAGER:
  - ozone.scm.db.dirs: /var/lib/hadoop-ozone/scm/data
  - ozone.metadata.dirs: /var/lib/hadoop-ozone/scm/ozone-metadata
  - ozone.scm.ha.ratis.storage.dir: /var/lib/hadoop-ozone/scm/ratis
YARN:
  NODEMANAGER:
    yarn_nodemanager_local_dirs: /yarn/nm
    yarn_nodemanager_recovery_dir: /var/lib/hadoop-yarn/yarn-nm-recovery
ZOOKEEPER:
  SERVER:
    dataDir: /var/lib/zookeeper
    dataLogDir: /var/lib/zookeeper
